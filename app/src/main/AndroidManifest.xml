<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sample.kirsten.fitme">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ARView">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data android:scheme="fitme2" android:host="loadthis" android:path="/"/>
            </intent-filter>
        </activity>
        <activity android:name=".Img8" />
        <activity android:name=".Img7" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Catalog" />
        <activity android:name=".Img1" />
        <activity android:name=".Img2" />
        <activity android:name=".Img3" />
        <activity android:name=".Img4" />
        <activity android:name=".Img5" />
        <activity android:name=".Img6" />
        <activity android:name=".Img7" />
        <activity android:name=".Img8" />
    </application>

</manifest>